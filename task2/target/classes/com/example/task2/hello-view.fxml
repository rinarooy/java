<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.task2.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="resText"/>
    <TextField fx:id="login" promptText="Login"/>
    <TextField fx:id="password" promptText="Password"/>
    <TextField fx:id="email" promptText="Email"/>
    <TextField fx:id="bYear" promptText="Birth Year"/>

    <Button text="Sign In" onAction="#onLoginButtonClick"/>
    <Button text="Registration" onAction="#onHelloButtonClick"/>

</VBox>
